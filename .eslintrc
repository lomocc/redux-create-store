{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "ecmaFeatures": {
        "objectLiteralComputedProperties": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console":1,
        "no-unused-vars":0,
        "no-alert":2,
        "no-eval":2,
        "semi": [1, "always"],
        "quotes": [1, "single"],

        "react/display-name": [ 1, {"ignoreTranspilerName": false }]
    }
}